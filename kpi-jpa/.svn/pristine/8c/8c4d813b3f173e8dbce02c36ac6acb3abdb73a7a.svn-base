package trong.lixco.com.jpa.entity;

import javax.persistence.Entity;
import javax.persistence.ManyToOne;

/**
 * @author vantrong
 * @Time 16-04-2018 12:08:21
 * Benh vien
 */
@Entity
public class Hospital extends AbstractEntity {

	
	private String code;
	private String name;
	private String addl;
	private String codeBH;
	@ManyToOne
	private City city;

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public City getCity() {
		return city;
	}

	public void setCity(City city) {
		this.city = city;
	}

	public String getAddl() {
		return addl;
	}

	public void setAddl(String addl) {
		this.addl = addl;
	}

	public String getCodeBH() {
		return codeBH;
	}

	public void setCodeBH(String codeBH) {
		this.codeBH = codeBH;
	}
	

}
