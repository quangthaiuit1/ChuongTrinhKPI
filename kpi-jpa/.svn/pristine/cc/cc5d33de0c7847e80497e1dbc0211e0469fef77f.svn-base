package trong.lixco.com.jpa.entity;

import javax.persistence.Entity;
/**
 * @author vantrong
 * @Time 16-04-2018 12:08:21
 * Thanh pho
 */
@Entity
public class City extends AbstractEntity {

	private String code;//ma thanh pho
	private String name; // ten thanh pho
	private String codeT;// ma theo thue
	private String codeBH;// ma theo bao hiem
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getCodeT() {
		return codeT;
	}
	public void setCodeT(String codeT) {
		this.codeT = codeT;
	}
	public String getCodeBH() {
		return codeBH;
	}
	public void setCodeBH(String codeBH) {
		this.codeBH = codeBH;
	}
	
}
