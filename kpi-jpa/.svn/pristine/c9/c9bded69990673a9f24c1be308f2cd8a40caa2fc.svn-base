package trong.lixco.com.jpa.entitykpi;

import javax.persistence.Entity;
import javax.persistence.ManyToOne;

import trong.lixco.com.jpa.entity.AbstractEntity;
import trong.lixco.com.jpa.entity.Employee;

/**
 * @author vantrong
 * @Time 16-04-2018 12:08:21 Chuc vu (trong KPI theo mo ta cong viec)
 */
@Entity
public class EmpPJob extends AbstractEntity {
	@ManyToOne
	private Employee employee;
	@ManyToOne
	private PositionJob positionJob;

	public EmpPJob() {

	}

	public EmpPJob(Employee emp, PositionJob pos) {
		this.employee = emp;
		this.positionJob = pos;
	}

	public Employee getEmployee() {
		return employee;
	}

	public void setEmployee(Employee employee) {
		this.employee = employee;
	}

	public PositionJob getPositionJob() {
		return positionJob;
	}

	public void setPositionJob(PositionJob positionJob) {
		this.positionJob = positionJob;
	}

}
